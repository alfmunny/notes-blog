<% if (index) { %>
    <% if (post.link) { %>
        <a class="<%= class_name %>" href="<%- url_for(post.link) %>" target="_blank" itemprop="url"><%= post.title %></a>
    <% } else if (post.title) { %>
        <a class="<%= class_name %>" href="<%- url_for(post.path) %>"><%= post.title %></a>
    <% } else { %>
        <a class="<%= class_name %>" href="<%- url_for(post.path) %>"><%= post.path.split("/")[3].split("-").join(" ") %></a>
    <% } %>
<% } else { %>
    <% if (post.title) { %>
        <h1 class="<%= class_name %>" >
            <%= post.title %>
    </h1>
    <% } else { %>
        <h1 class="<%= class_name %>"><%= post.path.split("/")[3].split("-").join(" ") %></h1>
    <% } %>
<% } %>

